class Solution(object):
    def numDistinct(self, s, t):
        """
        :type s: str
        :type t: str
        :rtype: int
        """
        dp=[0]*(len(t)+1)
        dp[0]=1
        for i in range(len(s)):
            pre=dp[0]
            for j in range(len(t)):
                if t[j]==s[i]:
                    cur=dp[j+1]+dp[j]
                else:
                    cur=dp[j+1]
                dp[j]=pre
                pre=cur
            dp[-1]=pre
        return dp[-1]
